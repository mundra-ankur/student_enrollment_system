<div id="<%= dom_id student %>">
  <p>
    <strong>Email:</strong>
    <%= student.email %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= student.name %>
  </p>

  <p>
    <strong>Date of birth:</strong>
    <%= student.date_of_birth %>
  </p>

  <p>
    <strong>Phone number:</strong>
    <%= student.phone_number %>
  </p>

  <p>
    <strong>Major:</strong>
    <%= student.major %>
  </p>

</div>
